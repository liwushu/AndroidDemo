<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gionee.apidemos"
    android:versionCode="1"
    android:versionName="1.0" >


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TopLayerBlurActivty"
            android:label="@string/top" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.gionee.intent.category.API_SAMPLE" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BlurActivity"
            android:label="@string/blur_activity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.gionee.intent.category.API_SAMPLE" />
            </intent-filter>
        </activity>
        
        <activity android:name="com.gionee.apidemos.TurnpagerActivity" 
            android:hardwareAccelerated="false"
		    android:label="@string/turnpager">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
                <category android:name="com.gionee.intent.category.API_SAMPLE" />
			</intent-filter>
		</activity>
		
		<activity android:name="com.gionee.apidemos.CycleTurnPagerActivity" 
		    android:hardwareAccelerated="false"
		    android:label="@string/cycleturnpager">
			<intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.gionee.intent.category.API_SAMPLE" />
            </intent-filter>
		</activity>
		
			
		<activity android:name="com.gionee.apidemos.ThirdEffectDemoActivity" 
		    android:label="@string/thirdeffect">
			<intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.gionee.intent.category.API_SAMPLE" />
            </intent-filter>
		</activity>
		
		 <activity
            android:name="com.gionee.apidemos.StageActivity"
            android:label="@string/clipeffect"
            android:screenOrientation="portrait">
			<intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.gionee.intent.category.API_SAMPLE" />
            </intent-filter>
        </activity>
        
    </application>
    
    <!-- <uses-permission android:name="android.permission.READ_FRAME_BUFFER" /> -->

</manifest>
